{
  "webservices": [
    {
      "name": "hello",
      "host": ":8001",
      "routes": [
        {
          "description": "creating web test service",
          "method": "GET",
          "route": "/hello",
          "response": {
            "status": 200,
            "body": {
              "message": "Hello friend!"
            }
          }
        }
      ]
    },
    {
      "name": "goodbye",
      "host": ":8002",
      "routes": [
        {
          "description": "creating web test service",
          "method": "GET",
          "route": "/goodbye",
          "response": {
            "status": 200,
            "body": {
              "message": "Goodbye friend!"
            }
          }
        }
      ]
    }
  ],
  "redis": [
    {
      "name": "redis",
      "configuration": {
        "protocol": "tcp",
        "addr": "redis:6379",
        "size": 10
      },
      "commands": [
        {
          "command": "DEL",
          "arguments": ["id"]
        },
        {
          "command": "DEL",
          "arguments": ["name"]
        },
        {
          "command": "APPEND", 
          "arguments": ["id", "1"]
        },
        {
          "command": "APPEND", 
          "arguments": ["name", "JOAO RIBEIRO"]
        }
      ]
    }
  ]
}
